source 'https://rubygems.org'

gem 'rails', '3.2.13'
gem 'bootstrap-sass'

group :development do
  gem 'sqlite3'
end

group :production, :rehearse do 
  gem 'mysql2'
end

# Gems used only for assets and not required
# in production environments by default.
group :assets do
  gem 'sass-rails',   '~> 3.2.3'
  gem 'coffee-rails'
  gem 'uglifier', '>= 1.0.3'
  gem 'bcrypt-ruby'
  gem 'will_paginate'
  gem 'bootstrap-will_paginate'
  gem 'octokit'
  gem 'paperclip'
end

gem 'jquery-rails'
