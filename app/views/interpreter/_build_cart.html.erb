// get cart

  var cart = [];

  <% current_user.cart_items.each do |c| %>

    <% if c.item != nil %>

      cart.push({
        id: <%= c.item.id %>,
        sample_name: '<%= c.item.sample.name %>',
        sample_type: '<%= c.item.sample.sample_type.name %>'
      });

    <% end %>
  
  <% end %>
