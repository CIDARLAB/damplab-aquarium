<tr>
   <td><%= user.login %></td>
   <td><%= link_to user.name, user %></td>
   <td><%= user.created_at.strftime("%m/%d/%Y") %></td>
   <td><% if current_user.is_admin && !current_user?(user) %>
    <%= link_to "delete", user, method: :delete,
                                  data: { confirm: "You sure?" } %></td>
  <% end %>
</tr>
