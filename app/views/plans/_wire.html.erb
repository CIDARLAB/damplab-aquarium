<polyline ng-attr-class="{{wire == current_wire ? 'wire-selected' : 'wire'}}"
          ng-mousedown="wireMouseDown($event, wire)" 
          ng-attr-points="{{wire.path}}">
</polyline>

<path ng-attr-class="{{wire == current_wire ? 'wire-arrowhead-selected' : 'wire-arrowhead'}}"
      ng-attr-d="{{wire.arrowhead}}">
</path>



